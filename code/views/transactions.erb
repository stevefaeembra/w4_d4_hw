<h1>Transactions</h1>
<table>
  <thead>
    <th>Date</th>
    <th>Notes</th>
    <th>Vendor</th>
    <th>Account</th>
    <th>Amount</th>
  </thead>
  <tbody>
    <% @xactions.each do |xaction| %>
      <tr>
        <td><%= xaction.dmy %></td>
        <td><%= xaction.notes %></td>
        <td><%= xaction.vendor_id %></td>
        <td><%= xaction.account_id %></td>
        <td class="amount"><%= xaction.amount %></td>
      </tr>
    <% end %>
  </tbody>
  <tfoot>

  </tfoot>
</table>
