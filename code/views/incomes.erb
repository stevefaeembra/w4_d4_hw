<nav>
  <a class="add" href="/incomes/new">New</a>
  <h2>Incomes</h2>
</nav>
<table>
  <thead>
    <th>ID</th>
    <th>Date</th>
    <th>Name</th>
    <th>Account</th>
    <th>Amount</th>
    <th></th>
  </thead>
  <tbody>
    <% @incomes.each do |income| %>
      <tr>
        <td><%= income.id %></td>
        <td><%= income.day_in_month %></td>
        <td><%= income.name %></td>
        <td><%= income.account_id %></td>
        <td class="amount"><%= income.amount %></td>
        <td><a class="delete" href="/incomes/<%= income.id %>/delete"><img class="icon" src="/images/throw-to-paper-bin.svg"></i></a></td>
      </tr>
    <% end %>
  </tbody>
  <tfoot>

  </tfoot>
</table>
